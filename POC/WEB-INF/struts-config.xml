<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
  <data-sources />
  <form-beans />
  <global-exceptions />
  <global-forwards />
  <action-mappings >
    <action
      path="/do/getReport"
      type="com.poc.struts.action.ReportAction"
      validate="false">
      <forward name="success" path="/jsp/displayReport.jsp" />
    </action>

  </action-mappings>

  <message-resources parameter="com.poc.struts.ApplicationResources" />
</struts-config>

